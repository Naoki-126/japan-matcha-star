
  <div class="breadcrumbs-area">
    <nav  role="navigation" aria-label="breadcrumb">
      <ol class="breadcrumb-wrap">
        <li class="breadcrumb-page"><a href="{{ routes.root_url }}">HOME</a></li>
        <li class="breadcrumb-page">{{ page.title }}</li>
      </ol>
    </nav>
  </div>

  <section id="js-faq"  class="jm-faq" >
    <div class="page-inner">
      <div class="content-inner">

        <div class="jm-faq__ttlArea">
          {% if section.settings.heading != blank %}
            <h2 class="jm-section__title">
              {{ section.settings.heading }}
            </h2>
          {% endif %}
          {% if section.settings.subheading != blank %}
            <p class="jm-section__subtitle">
              {{ section.settings.subheading | newline_to_br }}
            </p>
          {% endif %}
        </div>

      </div>
    </div>
  </section>


{% schema %}
{
  "name": "Page-head",
  "tag": "section",
  "disabled_on": {
    "groups": ["header", "footer"]
  },
  "settings": [
      {
      "type": "text",
      "id": "heading",
      "label": "見出し",
      "default": "Contact Form"
    },
    {
      "type": "textarea",
      "id": "subheading",
      "label": "サブ見出し",
      "default": "If you have any questions or concerns, please feel free to reach out to us."
    },
  ],
  "presets": [
    {
      "name": "Page-head",
      "category": "Page"
    }
  ]
}
{% endschema %}
